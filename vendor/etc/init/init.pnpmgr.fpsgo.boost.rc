on property:vendor.prop.pnp.fpsgo.state=disable_pending
    write /sys/power/pnpmgr/fpsgo_boost/boost_enable 0

on property:vendor.prop.pnp.fpsgo.boost=disable
    write /sys/power/pnpmgr/fpsgo_boost/boost_mode "disable_activity,disable_mode"

on property:vendor.prop.pnp.fpsgo.boost=enable
    write /sys/power/pnpmgr/fpsgo_boost/boost_enable 1
